<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="1080.0" spacing="10.0" styleClass="img-menu-background" stylesheets="@style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.simulator.hospital.view.MainMenuViewControl">
   <padding>
      <Insets bottom="50.0" left="100.0" right="100.0" top="50.0" />
   </padding>
   <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="title" text="Hospital Simulation" textAlignment="CENTER" wrappingWidth="741.8251953125">
      <VBox.margin>
         <Insets top="20.0" />
      </VBox.margin></Text>
   <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h1-text" text="Set quantity or/and process time (min)" wrappingWidth="854.8251953125">
   </Text>
   <GridPane>
      <columnConstraints>
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="538.0" minWidth="10.0" prefWidth="437.0"/>
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="450.0" minWidth="10.0" prefWidth="443.0"/>
      </columnConstraints>
      <rowConstraints>
         <RowConstraints maxHeight="65.0" minHeight="10.0" prefHeight="62.0" vgrow="SOMETIMES"/>
         <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
      </rowConstraints>
      <VBox.margin>
         <Insets bottom="20.0"/>
      </VBox.margin>
      <VBox alignment="CENTER" prefHeight="64.0" prefWidth="276.0">
         <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h2-text" text="Arrival " textAlignment="CENTER"
               wrappingWidth="111.8251953125">
         </Text>
         <TextField fx:id="arrivalTime" maxWidth="100.0" prefHeight="25.0" prefWidth="266.0" promptText="Enter time"/>
      </VBox>
      <VBox alignment="CENTER" GridPane.columnIndex="1">
         <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h2-text" text="Register Desk " textAlignment="CENTER"
               wrappingWidth="401.8251953125">
      </Text>
         <HBox alignment="CENTER" spacing="5.0">
            <ChoiceBox fx:id="registerChoice" prefHeight="25.0" prefWidth="115.0"/>
            <TextField fx:id="registerTime" prefHeight="25.0" prefWidth="100.0" promptText="Enter time"/>
         </HBox>
      </VBox>
      <VBox alignment="CENTER" GridPane.rowIndex="1">
         <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h2-text" text="General Examination"
               textAlignment="CENTER" wrappingWidth="307.8251953125">
      </Text>
         <HBox alignment="CENTER" prefHeight="25.0" prefWidth="175.0" spacing="5.0">
            <ChoiceBox fx:id="generalChoice" prefHeight="25.0" prefWidth="115.0"/>
            <TextField fx:id="generalTime" prefHeight="25.0" prefWidth="100.0" promptText="Enter time"/>
         </HBox>
      </VBox>
      <VBox alignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h2-text" text="Specialist Examination"
               textAlignment="CENTER" wrappingWidth="379.8251953125">
      </Text>
         <HBox alignment="CENTER" spacing="5.0">
            <ChoiceBox fx:id="specialistChoice" prefHeight="25.0" prefWidth="115.0"/>
            <TextField fx:id="specialistTime" prefHeight="25.0" prefWidth="100.0" promptText="Enter time"/>
         </HBox>
      </VBox>
   </GridPane>
   <HBox alignment="CENTER" spacing="50.0">
      <HBox alignment="CENTER" spacing="10.0">
         <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h1-text" text="Simulation time (min):" wrappingWidth="393.8251953125">
         </Text>
         <TextField fx:id="simulationTimeField" prefHeight="39.0" prefWidth="130.0" promptText="Enter time"/>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h1-text" text="Delay (s):" wrappingWidth="162.8251953125">
         </Text>
         <ChoiceBox fx:id="delayField" prefHeight="43.0" prefWidth="150.0" />
      </HBox>
   </HBox>
   <Button fx:id="startButton" mnemonicParsing="false" prefHeight="84.0" prefWidth="239.0" styleClass="start-button" text="START" textAlignment="CENTER" textFill="#1a1b1a">
      <VBox.margin>
         <Insets top="20.0" />
      </VBox.margin></Button>
</VBox>
